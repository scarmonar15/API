<h1>Listing counselings_students</h1>

<table>
  <thead>
    <tr>
      <th>Counseling</th>
      <th>Student</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @counselings_students.each do |counselings_student| %>
      <tr>
        <td><%= counselings_student.counseling.id unless counselings_student.counseling.blank? %></td>
        <td><%= counselings_student.student.full_name unless counselings_student.student.blank? %></td>
        <td><%= link_to 'Show', counselings_student %></td>
        <td><%= link_to 'Edit', edit_counselings_student_path(counselings_student) %></td>
        <td><%= link_to 'Destroy', counselings_student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Counselings student', new_counselings_student_path %>
