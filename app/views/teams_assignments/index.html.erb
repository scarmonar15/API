<h1>Listing teams_assignments</h1>

<table>
  <thead>
    <tr>
      <th>Team</th>
      <th>Assignment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teams_assignments.each do |teams_assignment| %>
      <tr>
        <td><%= teams_assignment.team %></td>
        <td><%= teams_assignment.assignment %></td>
        <td><%= link_to 'Show', teams_assignment %></td>
        <td><%= link_to 'Edit', edit_teams_assignment_path(teams_assignment) %></td>
        <td><%= link_to 'Destroy', teams_assignment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Teams assignment', new_teams_assignment_path %>
