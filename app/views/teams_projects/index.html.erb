<h1>Listing teams_projects</h1>

<table>
  <thead>
    <tr>
      <th>Team</th>
      <th>Project</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teams_projects.each do |teams_project| %>
      <tr>
        <td><%= teams_project.team.name unless teams_project.team.blank?  %></td>
        <td><%= teams_project.project.title unless teams_project.project.blank? %></td>
        <td><%= link_to 'Show', teams_project %></td>
        <td><%= link_to 'Edit', edit_teams_project_path(teams_project) %></td>
        <td><%= link_to 'Destroy', teams_project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Teams project', new_teams_project_path %>
